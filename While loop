import java.util.Scanner;

public class Main{
    public static void main(String[]args){
        Scanner in=new Scanner(System.in);
        System.out.println("Enter up to which number you want to print whole numbers:");
        int N=in.nextInt();
        int i=0;
        while(i<=N){
            System.out.println(i);
            i++;
        }
    }
}
